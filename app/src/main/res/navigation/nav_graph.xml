<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/settingsFragment">
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.example.passwordmanager.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings" >
        <action
            android:id="@+id/action_settingsFragment_to_securityActivity3"
            app:destination="@id/securityActivity3"
            app:enterAnim="@anim/slide_to_right"
            app:exitAnim="@anim/slide_from_left"
            app:popEnterAnim="@anim/slide_to_left"
            app:popExitAnim="@anim/slide_from_right" />
    </fragment>
    <activity
        android:id="@+id/securityActivity3"
        android:name="com.example.passwordmanager.SettingsBlocks.SecurityActivity"
        android:label="activity_security"
        tools:layout="@layout/activity_security" />
</navigation>